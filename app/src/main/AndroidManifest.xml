<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pierregmail.thube.tabataproject">

    <application
        android:name="com.orm.SugarApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="DATABASE"
            android:value="tabata_project.db" />
        <meta-data
            android:name="VERSION"
            android:value="2" />
        <meta-data
            android:name="QUERY_LOG"
            android:value="true" />
        <meta-data
            android:name="DOMAIN_PACKAGE_NAME"
            android:value="com.pierregmail.thube.tabataproject.data" />

        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.PreparationActivity" />
        <activity android:name=".Activity.ExerciceActivity" />
        <activity android:name=".Activity.ReposActivity" />
        <activity android:name=".Activity.CycleActivity" />
        <activity android:name=".Activity.TabataActivity" />
        <activity android:name=".Activity.LoadActivity" />
        <activity android:name=".Activity.StartActivity"></activity>
    </application>

</manifest>